    <clist name="persist.vivo.audio.feature.vc.magic_voicer_version">
        <!-- vc voicer version for vivo voice conversion, this parameter has a higher priority than
            {@code MagicVoice} in afs_config.xml, if this parameter is -1 or not configured, {@code MagicVoice} prevails -->
        <!-- MUST define a default key-value for audio policy implementation so that it works rightly! -->
        <item name="persist.vivo.audio.feature.vc.magic.voicer.version" value="-1"/>
    </clist>
