<?xml version="1.0" encoding="utf-8"?>
<AIDeglareEffectConfig version="0.0.4">
  <GlareRatio_Night_BackCam description="deglare strength parameter, ranges in [0, 1.0]">0.5</GlareRatio_Night_BackCam>
  <GlareRatio_Day_BackCam description="deglare strength parameter, ranges in [0, 1.0]">0.35</GlareRatio_Day_BackCam>
  <GlareRatio_Night_FrontCam description="deglare strength parameter, ranges in [0, 1.0]">0.3</GlareRatio_Night_FrontCam>
  <GlareRatio_Day_FrontCam description="deglare strength parameter, ranges in [0, 1.0]">0.15</GlareRatio_Day_FrontCam>

  <HighLightThres_Night_BackCam description="High Light Threshold, ranges in [0, 255]">230</HighLightThres_Night_BackCam>
  <HighLightThres_Day_BackCam description="High Light Threshold, ranges in [0, 255]">210</HighLightThres_Day_BackCam>
  <HighLightThres_Night_FrontCam description="High Light Threshold, ranges in [0, 255]">230</HighLightThres_Night_FrontCam>
  <HighLightThres_Day_FrontCam description="High Light Threshold, ranges in [0, 255]">230</HighLightThres_Day_FrontCam>

  <GlareGuidedFilterParams description="parameteres for glare filtering">
    <Radius description="half length of a local square area, which would be filtered when filtering the center pixel">220</Radius>
    <Scale description="downscale parameter, the parameter increase would speed up the filtering, but may cause quality decrease">4</Scale>
    <Eps description="filter strength, the parameter increse would make the image more blurry">0.01</Eps>
    <Mode description="guided filter mode">256</Mode>
  </GlareGuidedFilterParams>
  <MaskGuidedFilterParams description="parameteres for highlight mask filtering, the paramter descriptions are the same with the above">
    <Radius>20</Radius>
    <Scale>4</Scale>
    <Eps>0.001</Eps>
    <Mode>0</Mode>
  </MaskGuidedFilterParams>
</AIDeglareEffectConfig>